class Solution {
    public boolean repeatedSubstringPattern(String s) {
        int n=s.length();
      for(int i=n/2; i>0; i--){
          if(n%i==0){
              int left=0;
          int right=i;
              while(right<n && s.charAt(left++)==s.charAt(right)) right++;
              if(right==n) return true;
          }
         
          
      }  
        return false;
    }
}
#Status: Solved
