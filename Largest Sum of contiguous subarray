class Solution {
    public int maxSubArray(int[] nums) {
       int maxi=0;
        int max=Integer.MIN_VALUE;
        for(int i=0; i<nums.length; i++){
            maxi+=nums[i];
            if(maxi>max) max=maxi;
            else if(maxi<0) maxi=0;
        }
        return max;
    }
}
#Status: Solved
